cp * ../

python modifyParam.py


make clean
make -j -f Makefile-fp32-cgfdm
cp params.json_FP32-CGFDM params.json
bash FP32-CGFDM-run


make clean
make -j -f Makefile-fp16-cgfdm
cp params.json_FP16-CGFDM params.json
bash FP16-CGFDM-run	


make clean
make -j -f Makefile-fp32-sve
cp params.json_FP32-SVE params.json
bash FP32-SVE-run


make clean
make -j -f Makefile-fp16-sve
cp params.json_FP16-SVE params.json
bash FP16-SVE-run

